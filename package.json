{
  "name": "@funkybob/jwt",
  "version": "2.0.3",
  "description": "Native JWT validation without deps",
  "main": "dist/index.js",
  "module": "index.js",
  "files": [
    "src/*",
    "dist/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "prepare": "npm run build && agadoo && npm run docs",
    "docs": "esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funkybob/funky-jwt.git"
  },
  "author": "Curtis Maloney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funkybob/funky-jwt/issues"
  },
  "homepage": "https://github.com/funkybob/funky-jwt#readme",
  "devDependencies": {
    "agadoo": "^2.0.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-terser": "^5.2.0"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  },
  "dependencies": {}
}
